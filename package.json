{
    "name": "ical-today-fetcher",
    "version": "1.0.0",
    "description": "Fetch all events for today for all given ics/ical urls",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "test": "jest"
    },
    "keywords": [
        "ics",
        "ical",
        "today",
        "calendar",
        "event",
        "events",
        "schedule",
        "online"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Robin7077/ical-today-fetcher.git"
    },
    "author": "Robin7077",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^24.0.13",
        "@types/node": "^12.0.4",
        "jest": "^24.8.0",
        "jest-cli": "^24.8.0",
        "ts-jest": "^24.0.2",
        "tslint": "^5.17.0",
        "typescript": "^3.5.1"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "ical-expander": "^2.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": "\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "collectCoverageFrom": [
            "**/*.{ts,js}",
            "!**/*.testdata.ts"
        ],
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "clover",
            "text-summary"
        ],
        "testEnvironment": "node"
    }
}
